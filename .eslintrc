{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "max-len": 0,
    "react/prefer-es6-class": 0,
    "react/no-multi-comp": 0
  }
}
